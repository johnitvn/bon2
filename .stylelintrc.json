{
  "extends": ["stylelint-config-standard"],
  "plugins": ["stylelint-scss", "stylelint-less"],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["tailwind"]
      }
    ],
    "color-no-invalid-hex": true,
    "less/color-no-invalid-hex": true
  }
}
